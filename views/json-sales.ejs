<div class="container">
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">name</th>
      <th scope="col">age</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i of sales){ %>
      <tr>
        <td><%= i.id %></td>
        <td><%= i.name %></td>
        <td><%= i.age %></td>
      </tr>
    <% } %>
  </tbody>
</table>
</div>
